OPTIONS {
Summary: 0;
}

POSITIVE_VARIABLES ONE_VAR_CONST__;

VARIABLES x1, x2, x3;

LOWER_BOUNDS{
x1: -1;
x2: -1;
x3: -1;
}

UPPER_BOUNDS{
x1: 1;
x2: 1;
x3: 1;
}

EQUATIONS c_e_FIX_ONE_VAR_CONST__, c1, c2, c3, c4, c5, c6;

c_e_FIX_ONE_VAR_CONST__:  ONE_VAR_CONST__  == 1;
c1: 1/2 * x1 * (x2 - x3) == 2;
c2: x1/2 * (x2 - x3) == 2;
c3: x1 * (x2 - x3)/2 == 2;
c4: x1 * (x2/2 - x3/2) == 2;
c5: x1 * (x2 - x3) * (1/2) == 2;
c6: x1 * (x2 - x3) + (-2) * 2 == 0;

OBJ: minimize x2;

STARTING_POINT{
ONE_VAR_CONST__: 1;
x1: 3;
x2: 1;
x3: 2;
}

