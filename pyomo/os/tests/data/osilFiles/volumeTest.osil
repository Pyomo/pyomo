<?xml version="1.0" encoding="UTF-8"?>
<osil xmlns="os.optimizationservices.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="os.optimizationservices.org http://www.optimizationservices.org/schemas/2.0/OSiL.xsd">
	<instanceHeader>
		<name>volumeTest </name>
		<source>Kipp Martin</source>
		<description> Trival Problem to test Volume Algorithm </description>
	</instanceHeader>
	<instanceData>
		<variables numberOfVariables="2">
			<var name="x0" lb="-1000" ub = "1000"  />
			<var name="x1" lb="-1000"  ub = "1000" />
		</variables>
		<objectives numberOfObjectives="1">
			<obj name = "Par, Inc. Objective Function" maxOrMin="min"  numberOfObjCoef="2">
				<coef idx="0">4</coef>
				<coef idx="1">3</coef>
			</obj>
		</objectives>
		<constraints numberOfConstraints="2"> 
			<con name="con1"  lb="1" ub="1" />
			<con name="con2" lb="1" ub="1"/>
		</constraints>
		<linearConstraintCoefficients numberOfValues="2">
			<start>
				<el>0</el>
				<el>1</el>
				<el>2</el>
			</start>
			<rowIdx>
				<el>0</el>
				<el>1</el>
			</rowIdx>
			<value>
			<el>1</el>
				<el>1</el>
			</value>
		</linearConstraintCoefficients>
	</instanceData>
</osil>
