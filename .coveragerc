[report]
omit =
    setup.py
    */tests/*
    */tmp/*

# The [run] section must be at the end, as the build harness will add a
# "data_file" directive to the end of this file.
[run]
parallel = True
source =
    pyomo
    examples
omit =
    # github actions creates a cache directory we don't want measured
    cache/*
